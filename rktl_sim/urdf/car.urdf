<?xml version="1.0"?> 
<robot name="simplecar">
    <!-- Colors --> 
    <material name="black"> 
        <color rgba="0 0 0 1"/>
    </material>
    <material name="blue"> 
        <color rgba="0.6 0.7 0.8 1"/>
    </material>

    <!-- Base frame of car --> 
    <!-- Local frame is X up, Y left, and Z out -->
    <link name="base_link">
        <inertial> 
            <mass value="0"/>
            <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0.0" izz="0.2"/>
        </inertial> 
    </link>

    <link name="car_link">
        <visual> 
            <geometry> 
                <box size="0.13 0.28 0.1"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <geometry>
                <box size="0.13 0.28 0.1"/>
            </geometry>
        </collision>
        <inertial> 
            <mass value="6"/>
            <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0.0" izz="0.2"/>
        </inertial> 
    </link>

    <joint name="w_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 0 1" />
        <parent link="w_link" />
        <child link="car_link" />
    </joint>

    <link name="w_link">
        <inertial> 
            <mass value="0"/>
            <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0.0" izz="0.2"/>
        </inertial> 
    </link>

    <joint name="x_joint" type="prismatic">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="0 1 0" />
        <parent link="x_link" />
        <child link="w_link" />
        <limit effort="500" velocity="500.0" lower="-100.0" upper="100.0" />
    </joint>

    <link name="x_link">
        <inertial> 
            <mass value="0"/>
            <inertia ixx="0.2" ixy="0" ixz="0" iyy="0.2" iyz="0.0" izz="0.2"/>
        </inertial> 
    </link>

    <joint name="y_joint" type="prismatic">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <axis xyz="1 0 0" />
        <parent link="base_link" />
        <child link="x_link" />
        <limit effort="500" velocity="500.0" lower="-100.0" upper="100.0" />
    </joint>
</robot>